{
  "version": 2,
  "env": {
    "DATABASE_URL": "file:./dev.db"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "functions": {
    "server.js": {
      "includeFiles": ["prisma/**", "src/**"]
    }
  }
}
